{
  "include": [
    "src/**/*.js"
  ],
  "lines": 95,
  "statements": 95,
  "functions": 95,
  "branches": 95,
  "reporter": [
    "lcov",
    "text-summary",
    "html"
  ],
  "require": [
    "babel-register"
  ],
  "cache": true,
  "all": true,
  "check-coverage": true,
  "sourceMap": false,
  "instrument": false
}
